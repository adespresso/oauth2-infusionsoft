{
    "name": "adespresso/oauth2-infusionsoft",
    "description": "Infusionsoft OAuth 2 Client Provider for The PHP League OAuth2-Client package",
    "license": "MIT",
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "infusionsoft"
    ],
    "require": {
        "league/oauth2-client": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "AdEspresso\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdEspresso\\OAuth2\\Client\\Tests\\": "tests/src/"
        }
    }
}
